#ifndef HARDWARE_PWM_H_
#define HARDWARE_PWM_H_
#include "DSP28x_Project.h"
typedef enum{
    EPWMA,
    EPWMB
}enum_EPWM;
#define SD(x) if(x) GpioDataRegs.GPASET.bit.GPIO11 = 1;\
        else GpioDataRegs.GPACLEAR.bit.GPIO11 = 1
void pwmInit(Uint16 tbprd);
void setCompare(enum_EPWM ePwmx,Uint16 period);
void adjustVoltage(Uchar8 status);
